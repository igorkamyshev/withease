{
  "name": "@withease/redux",
  "version": "1.1.1",
  "license": "MIT",
  "peerDependencies": {
    "effector": "^22.8.8 || ^23.0.0",
    "redux": "^4.0.0 || ^5.0.0"
  },
  "scripts": {
    "test:run": "vitest run --typecheck",
    "build": "vite build",
    "size": "size-limit",
    "typelint": "attw --pack"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/redux.cjs",
  "module": "./dist/redux.js",
  "types": "./dist/redux.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/redux.d.ts",
        "default": "./dist/redux.js"
      },
      "require": {
        "types": "./dist/redux.d.cts",
        "default": "./dist/redux.cjs"
      }
    }
  },
  "size-limit": [
    {
      "path": "./dist/redux.js",
      "limit": "569 B"
    }
  ]
}
